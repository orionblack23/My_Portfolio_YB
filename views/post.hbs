
<!--========== Start Wrapper ==========-->
<div class="wrapper">

    <!--========== Start Header ==========-->
    <header class="lrs-header">
        <!--========== Start Navigation ==========-->
        {{>navBar}}
        <!--========== End Navigation ==========-->
    </header>
    <!--========== End Header ==========-->
    <div class="lrs-divider-3"></div>

    <!--========== Start Content Page ==========-->
    <div class="lrs-content-post">
        <!--========== Start Content Post ==========-->
    {{#with items}}
        <div class="container">
            <div class="row">
                <div class="lrs-post-page">
                    <div class="col-sm-8">
                        <div class="row">
                            <div class="lrs-post-page-img">
                                <img src="/images/92.jpg" class="img-responsive" alt="Resume / CV"/>
                            </div>
                            <div class="lrs-post-page-content">
                                <span class="lrs-post-page-category"><i>{{this.category}}</i></span>
                                <h2 class="lrs-post-page-title">
                                    {{this.title}}
                                </h2>
                                <br/>
                                <p>
                                    {{this.content}}
                                </p>
                                <br>
                                <blockquote class="style_1">
                                    {{this.authorSitation}}
                                    <span class="author">- {{this.author}}</span>
                                </blockquote>
                                <p>
                                    {{this.conclusion}}
                                </p>
                            </div>
                            <div class="lrs-divider-3"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {{/with}}
        <!--========== End Content Post ==========-->

        <div class="lrs-divider-3"></div>

<!--========== Start Content Contact Section ==========-->
{{#with section7}}
    <section class="lrs-angle" id="contact">
        <div class="lrs-angle-div">
            <div class="container">
                <div class="row">
                    <div class="lrs-heading-wrap">
                        <h3 class="lrs-heading-title">
                            <span class="nr-heading-title">06.</span> {{headingTitle}}
                        </h3>
                    </div>
                    <div class="lrs-divider-3"></div>
                    <div class="email">
                        <h5 class="email"  data-toggle="modal" data-target="#modal_contact">
                            <b>{{email}}</b>
                            <span class="click_email"><i class="fa fa-long-arrow-left"></i> Click here!</span>
                        </h5>
                    </div>
                    <h5 class="phone">
                        <b>{{phone}}</b>
                    </h5>
                    <ul class="list-unstyled list-inline social">
                        <!--<li><a href="#"><i class="fa fa-facebook-official"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>-->
                        <li><a target="_blank" href="{{social1}}"><i class="fa {{social1Font}}"></i></a></li>
                        <li><a target="_blank" href="{{social2}}"><i class="fa {{social2Font}}"></i></a></li>
                        <!--<li><a href="#"><i class="fa fa-pinterest"></i></a></li>
                        <li><a href="#"><i class="fa fa-youtube"></i></a></li>-->
                    </ul>
                    <p class="copy_rights">Â© 2019 Orion Black. All rights reserved.</p>
                </div>
            </div>
        </div>
    </section>
{{/with}}
    <!--========== End Content Contact Section ==========-->

<!--========== Start Modal Contact ==========-->
<div class="modal fade" id="modal_contact" tabindex="-1" role="dialog" aria-labelledby="modal_contact">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="lrs-divider-3"></div>
            <h4><b>Contact Me</b></h4>
            <div class="lrs-divider-3"></div>
            <form action="/post/send" method="POST" class="form_contact">
                <div class="input-group">
                    <input type="text" name="name" class="form-control" placeholder="Name">
                </div>
                <div class="input-group">
                    <input type="email" name="email" class="form-control" placeholder="Email">
                </div>
                <div class="input-group">
                    <input type="text" name="subject" class="form-control" placeholder="Subject">
                </div>
                <div class="input-group">
                    <textarea name="message" name="message" class="form-control"  id="message" placeholder="Message"></textarea>
                </div>
                <div class="input-group">
                    <input type="submit" class="submit pull-right" value="Send"/>
                </div>
            </form>
            <p class="btn-modern dismiss-modal" data-dismiss="modal">X</p>
        </div>
    </div>
</div>
<!--========== End Modal Contact ==========-->

<script type="text/javascript" src="/javascripts/jquery-3.1.0.min.js"></script>
<script type="text/javascript" src="/javascripts/modernizr.js"></script>
<script type="text/javascript" src="/javascripts/bootstrap.min.js"></script>

<script src="/javascripts/html5shiv.js" type="text/javascript"></script>
<script src="/javascripts/respond.min.js" type="text/javascript"></script>

<script type="text/javascript" src="/javascripts/mansory.js"></script>
<script type="text/javascript" src="/javascripts/gallery.js"></script>
<script type="text/javascript" src="/javascripts/script.min.js"></script>
